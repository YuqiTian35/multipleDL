library(dplyr)
library(multipleDL)
n <- 100
x <- rnorm(n * 3)
e <- rnorm(n * 3)
y <- exp(x + e)
no_dl <- 1e6
data <- data.frame(y = y, x = x, subset = rep(c(1, 2, 3), each=n))
data <- data %>%
mutate(dl_l = case_when(subset == 1 ~ 0.2,
subset == 2 ~ 0.3,
subset == 3 ~ -no_dl),
dl_u = case_when(subset == 1 ~ no_dl,
subset == 2 ~ 4,
subset == 3 ~ 3.5),
delta_l = ifelse(y >= dl_l, 1, 0),
delta_u = ifelse(y <= dl_u, 1, 0),
z = ifelse(!delta_l, dl_l, ifelse(!delta_u, dl_u, y)))
data_original <- data
data <- data_original %>% dplyr::select(x, z, delta_l, delta_u)
new <- multipleDL(formula = z ~ x, data = data,
delta_lower = data$delta_l, delta_upper = data$delta_u, link='probit')
new
